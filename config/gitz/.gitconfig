[init]
	defaultBranch = main
[core]
	editor = nvim
[alias]
  c = !git add --all git commit -m
  s = !git status -s
  l = !git log --pretty=format:'%C(blue)%h%C(red)%d %C(white)%s - %C(cyan)%cn, %C(green)%cr'
  ammend = !git add --all && git commit --amend --no-edit
  count = !git shortlog -s --grepa
